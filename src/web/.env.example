# API Configuration
# Base URL for API requests - defaults to local development
VITE_API_BASE_URL=http://localhost:8000/api/v1

# API Request Settings
# Timeout for API requests in milliseconds (30 seconds)
VITE_API_TIMEOUT=30000
# Number of retry attempts for failed API requests
VITE_API_RETRY_ATTEMPTS=3
# Delay between retry attempts in milliseconds (1 second)
VITE_API_RETRY_DELAY=1000

# AWS Cognito Authentication Configuration
# User Pool ID from AWS Cognito Console - @version: aws-sdk 2.1234.0
VITE_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxx
# Client ID from AWS Cognito Console
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
# AWS Region where Cognito resources are deployed
VITE_COGNITO_REGION=us-east-1
# Custom domain for Cognito OAuth flows
VITE_COGNITO_OAUTH_DOMAIN=auth.priorauth.com

# Security Settings
# Enable/disable Multi-Factor Authentication
VITE_MFA_ENABLED=true
# Preferred MFA method (SOFTWARE_TOKEN_MFA, SMS_MFA)
VITE_MFA_PREFERRED_METHOD=SOFTWARE_TOKEN_MFA
# Session duration in seconds (1 hour)
VITE_SESSION_DURATION=3600

# Environment Configuration
# Current environment (development/staging/production)
VITE_ENVIRONMENT=development
# Application logging level (debug/info/warn/error)
VITE_LOG_LEVEL=info

# Feature Flags
# JSON string containing feature flag configuration
# Example: {"newFeature": true, "betaFeature": false}
VITE_FEATURE_FLAGS={}

# IMPORTANT: This is an example configuration file
# Create a .env file based on this template with actual values
# DO NOT commit .env files containing real credentials to version control