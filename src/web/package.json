    "fhirpath": "^3.0.0",
    "form-data": "^4.0.0",
    "framer-motion": "^10.12.0",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "opossum": "^6.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.0.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.0",
