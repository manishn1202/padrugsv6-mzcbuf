    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-mock-console": "^2.0.0",
    "lint-staged": "^13.2.2",
    "msw": "^1.3.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vite-plugin-checker": "^0.6.0",
    "vitest": "^0.34.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
